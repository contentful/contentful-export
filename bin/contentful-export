#!/usr/bin/env node
var runContentfulExport = require('../dist/index')
var usageParams = require('../dist/usageParams')

runContentfulExport(usageParams)
  .then((result) => {
    process.exit(0)
  })
  .catch((err) => {
    if (err.name !== 'ContentfulMultiError') {
      console.error(err)
    }
    process.exit(1)
  })

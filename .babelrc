{
  "presets": [
    ["env", {
      "debug": true,
      "targets": {
        "node": 4.7,
        "browsers": [
          "last 3 versions",
          "ie >= 11"
        ]
      }
    }]
  ],
  "plugins": [
    "add-module-exports"
  ],
  "env": {
    "test": {
      "sourceMaps": "inline",
      "plugins": ["babel-plugin-rewire"]
    }
  }
}
